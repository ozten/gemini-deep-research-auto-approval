{
  "manifest_version": 3,
  "name": "Gemini Deep Research Auto-Start",
  "version": "1.0",
  "description": "Automatically starts Gemini Deep Research and then disables itself.",
  "permissions": ["storage", "scripting"],
  "host_permissions": ["https://gemini.google.com/app"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://gemini.google.com/app*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "icons/disabled.png",
      "48": "icons/disabled.png",
      "128": "icons/disabled.png"
    },
    "default_title": "Enable Auto-Start"
  },
  "icons": {
    "16": "icons/disabled.png",
    "48": "icons/disabled.png",
    "128": "icons/disabled.png"
  }
}
